{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "buildHooks": {
        "preBuild": "bash ./eas-build-pre-install.sh"
      }
    },
    "preview": {
      "distribution": "internal",
      "buildHooks": {
        "preBuild": "bash ./eas-build-pre-install.sh"
      }
    },
    "production": {
      "autoIncrement": true,
      "buildHooks": {
        "preBuild": "bash ./eas-build-pre-install.sh"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
