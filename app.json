{
  "expo": {
    "name": "FocusFit",
    "slug": "focusfit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.corygold.2minutefit",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.corygold.twominutefit"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "extraPods": [
              {
                "name": "RCT-Folly",
                "podspec": "../node_modules/react-native/third-party-podspecs/RCT-Folly.podspec"
              }
            ],
            "podfileProperties": {
              "EXPO_POD_REPO_UPDATE": "true"
            }
          }
        }
      ],
      "expo-audio",
      "expo-video",
      "expo-web-browser"
    ],
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "1fd6126a-4eb8-4f4b-a278-1d5c2ded5ba0"
      }
    },
    "owner": "corygold"
  }
}
